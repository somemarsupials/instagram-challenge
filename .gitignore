# Ignore bundler config.
instagram/.bundle

# Ignore all logfiles and tempfiles.
instagram/log/*
instagram/tmp/*
instagram!/log/.keep
instagram!/tmp/.keep

instagram/node_modules
instagram/yarn-error.log

instagram/.byebug_history

instagram/public/uploads/**/*
/**/.DS_Store
